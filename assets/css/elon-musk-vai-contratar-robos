	document.addEventListener('DOMContentLoaded', function (e) {
	  let id = `fed157c3-6dbd-47b7-aa07-8f3281af8dad`
	  let container = document.querySelector(`#gallery-fed157c3-6dbd-47b7-aa07-8f3281af8dad .gallery-mosaic-container`)
	  let numberIntervalAd = Number.parseInt("2")
  
	  window.photogallery = window.photogallery || {}
	  
	  window.photogallery["ads"] = window.photogallery["ads"] || {}
	  window.photogallery["ads"].adsLoaded = []
	  window.photogallery["ads"].numberIntervalAd = numberIntervalAd
	  window.photogallery["ads"].numberNextPub = window.photogallery["ads"].numberNextPub || 1
	  window.photogallery["ads"].numberFirstPubToLoad = window.photogallery["ads"].numberNextPub || 1
	  
	  window.photogallery[id] = window.photogallery[id] || {}
	  window.photogallery[id].gallery = new window.PhotoGallery(container.parentNode, window.photogallery["ads"])
  
	  container.addEventListener('click', e => {
		const photogallery = window.photogallery[id].gallery
		if (!photogallery.isGalleryOpened()) {
		  let positionPhoto = parseInt(e.target.dataset.posicao) || 3
		  positionPhoto = positionPhoto === (numberIntervalAd+1) ? positionPhoto+1 : positionPhoto 
		  photogallery.open(positionPhoto)
		}
	  }, true)
  
		function toggleCaptionExpansion() {
			const captions = document.querySelectorAll('.caption-expand');

			captions.forEach(caption => {
				caption.addEventListener('click', () => {
					const captionText = caption.textContent.trim();
					const maxLength = 100;
			
					if (captionText.length > maxLength) {
						caption.classList.toggle('expanded');
					}
				});
			});
		}
	
		toggleCaptionExpansion();
	});
