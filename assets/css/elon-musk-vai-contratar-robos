              googletag.cmd.push(function () {
                apstag.setDisplayBids();
                requestManager.aps = true; // signals that APS request has completed
              });
            }
          );
        }
      }
      // initiate bid request
      requestHeaderBids();
    }
    executeParallelAuctionAlongsidePrebid();
</script><script src="https://s3.glbimg.com/cdn/libs/jquery/1.8.3/jquery.min.js" type="text/javascript" defer crossorigin="anonymous"></script><script src="https://cdn.ampproject.org/v0/amp-twitter-0.1.js" type="text/javascript" async custom-element="amp-twitter"></script><script src="https://cdn.ampproject.org/v0/amp-facebook-0.1.js" type="text/javascript" async custom-element="amp-facebook"></script><script src="https://cdn.ampproject.org/v0/amp-instagram-0.1.js" type="text/javascript" async custom-element="amp-instagram"></script><script src="https://cdn.ampproject.org/v0/amp-tiktok-0.1.js" type="text/javascript" async custom-element="amp-tiktok"></script><link href="https://fonts.googleapis.com/css2?family=Source+Sans+Pro:wght@400;500;600;700&display=swap" type="text/css" rel="stylesheet"><style>
